{
    "rewrites": [
      { "source": "/submit-form/(.*)", "destination": "/index.html" }
    ],
    "builds": [
      {
        "src": "build/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  